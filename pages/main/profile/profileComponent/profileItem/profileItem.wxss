/* pages/main/profile/profileComponent/profileItem/profileItem.wxss */

.icon-item {
  width: 100%;
  height: 100rpx;
  box-sizing: border-box;
  padding: 0 20rpx;

  display: flex;
  align-items: center;
}

.icon-item image {
  width: 60rpx;
  height: 60rpx;
  margin-right: 20rpx;
}

.icon-item text {
  height: 100%;
  flex: 1;

  display: flex;
  align-items: center;
}
